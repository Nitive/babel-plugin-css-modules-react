    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-react": "^6.9.0",
    "hide-stack-frames-from": "^1.0.0",
    "mocha": "^3.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13"
  },
