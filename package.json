    "src",
    "example",
    "dist.js"
  ],
  "homepage": "https://github.com/nitive/babel-plugin-css-modules-react#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
